{
  "name": "sencrop-js-api-client",
  "version": "1.0.0",
  "description": "The Sencrop JavaScript API client",
  "main": "dist/src/index.js",
  "metapak": {
    "configs": [
      "main",
      "readme",
      "jsarch",
      "jsdocs",
      "oss"
    ],
    "data": {
      "files": "src/**/*.js bin/*.js test/*.js",
      "ignore": []
    }
  },
  "scripts": {
    "architecture": "jsarch src/**/*.js bin/*.js test/*.js > ARCHITECTURE.md",
    "build": "node bin/build.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "cli": "env NODE_ENV=${NODE_ENV:-cli}",
    "compile": "babel src/*.js --out-dir dist",
    "cover": "jest --coverage",
    "cz": "env NODE_ENV=${NODE_ENV:-cli} git cz",
    "doc": "mkdir -p .readme; echo \"# API\" > .readme/API.md; jsdoc2md src/**/*.js bin/*.js test/*.js >> .readme/API.md",
    "import": "node bin/import.js",
    "lint": "eslint src/**/*.js bin/*.js test/*.js",
    "metapak": "metapak || exit 0",
    "postinstall": "npm run metapak --silent",
    "preversion": "npm run compile && npm run lint && npm t",
    "test": "npm run compile && jest --config=.jest.config.js && karma start karma.conf.js",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sencrop/sencrop-js-api-client.git"
  },
  "keywords": [
    "API",
    "Sencrop"
  ],
  "author": "Nicolas Froidure (Sencrop)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sencrop/sencrop-js-api-client/issues"
  },
  "homepage": "https://github.com/sencrop/sencrop-js-api-client#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "camel-case": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^14.4.0",
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "^1.2.0",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "3.16.0",
    "eslint-config-simplifield": "4.1.1",
    "istanbul": "0.4.5",
    "jasmine-core": "^2.7.0",
    "jest": "20.0.4",
    "jsarch": "1.2.1",
    "jsdoc-to-markdown": "^3.0.0",
    "karma": "^1.7.0",
    "karma-browserify": "^5.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "metapak": "0.0.21",
    "metapak-sencrop": "2.0.0",
    "moxios": "^0.4.0",
    "nock": "^9.0.14",
    "sinon": "2.3.8",
    "swagger-http-router": "^0.1.0",
    "watchify": "^3.9.0"
  },
  "engines": {
    "node": ">=6.10.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}